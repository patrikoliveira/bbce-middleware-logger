{
  "name": "middleware-logger",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "compile": "rm -rf dist && npx tsc",
    "deploy": "npm run build:ts && npm run build:js && npm run build:min",
    "build:ts": "node_modules/.bin/tsc -b",
    "build:js": "node_modules/.bin/rollup -c rollup.config.js --environment BUILD:development",
    "build:min": "node_modules/.bin/rollup -c rollup.config.js --environment INCLUDE_DEPS,BUILD:minify"
  },
  "repository": "git@github.com:patrikoliveira/bbce-middleware-logger.git",
  "author": "Patrik Oliveira <patrik.oliveira@rarolabs.com.br>",
  "license": "proprietary",
  "private": true,
  "dependencies": {
    "@types/on-finished": "^2.3.1",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "axios": "^0.19.2",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "on-finished": "^2.3.0",
    "typedi": "^0.8.0",
    "uuidv4": "^6.0.7"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.4",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
